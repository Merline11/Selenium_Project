<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Parameters">
    <parameter name="browser" value="chrome"></parameter>
    <test verbose="2" preserve-order="true" name="Learning Parameterization">
        <parameter name="first-name" value="Abraham"></parameter>
        <parameter name="last-name" value="Lincoln"></parameter>
        <parameter name="city" value="New York"></parameter>
        <parameter name="dob" value="1-Jan-1901"></parameter>
        <classes>
            <class name="uk.axone.testngadvanced.TestHowToParameterize"></class>
        </classes>
    </test>
</suite>